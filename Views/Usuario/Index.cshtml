@model List<Proyecto.Models.Usuario>


<hi>Usuario</hi>

<p>
	<a asp-action="Create">Crear</a>
</p>

<table class="table">
	<thead>
		<tr>
			<th>Cedula</th>
			<th>Nombre</th>
			<th>Apellido 1</th>
			<th>Apellido 2</th>
			<th>Rol</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var usuario in Model)
		{
			<tr>
				<td>@Html.DisplayFor(m=>usuario.Cedula)</td>
				<td>@Html.DisplayFor(m=>usuario.Nombre)</td>
				<td>@Html.DisplayFor(m=>usuario.Apellido1)</td>
				<td>@Html.DisplayFor(m=>usuario.Apelllido2)</td>
				<td>@Html.DisplayFor(m=>usuario.Rol)</td>
			</tr>
		}
	</tbody>
</table>